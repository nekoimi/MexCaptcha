        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MexCaptcha\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "yprisoner",
            "email": "yyprisoner@gmail.com"
        }
